{
  "name": "InsureMate",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Get Insurance",
  "scripts": {
    "dev": "NODE_OPTIONS='--disable-warning=DEP0190' nitrostack-cli dev",
    "build": "NODE_OPTIONS='--disable-warning=DEP0190' nitrostack-cli build",
    "start": "npm run build && NODE_OPTIONS='--disable-warning=DEP0190' nitrostack-cli start",
    "start:prod": "NODE_OPTIONS='--disable-warning=DEP0190' nitrostack-cli start",
    "upgrade": "NODE_OPTIONS='--disable-warning=DEP0190' nitrostack-cli upgrade",
    "install:all": "NODE_OPTIONS='--disable-warning=DEP0190' nitrostack-cli install",
    "widget": "NODE_OPTIONS='--disable-warning=DEP0190' npm --prefix src/widgets"
  },
  "dependencies": {
    "@nitrostack/core": "^1",
    "dotenv": "^16.3.1",
    "mongodb": "^7.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nitrostack/cli": "^1",
    "@types/node": "^22.10.0",
    "typescript": "^5.3.3"
  },
  "author": "Geetha Charu Mathi"
}
